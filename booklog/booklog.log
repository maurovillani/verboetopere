function recursive1($array, $level = 0) {
    echo PHP_EOL;
    foreach ($array as $node) {
        echo str_repeat("&emsp;", $level), "[" . $node['name'] . "]", '';
        if (isset($node['children'])) {
            echo '<br/>';
            recursive1($node['children'], $level + 1);
            echo str_repeat("&emsp;", $level);
        }
        echo "[/" . $node['name'] . "]", '<br>'.PHP_EOL;
    }
}

